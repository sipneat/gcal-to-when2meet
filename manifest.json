{
    "name": "Google Calendar to when2meet",
    "version": "1.0.1",
    "description": "Automatically fill in when2meet availability from Google Calendar events",
    "manifest_version": 3,
    "author": "SipnEat",
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "identity",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_title": "Google Calendar to when2meet"
    }
}